{
  "name": "learning-speaking-app",
  "version": "0.2.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "echo 'No tests configured yet' && exit 0",
    "prepare": "husky install",
    "db:seed": "npx prisma db seed",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "qr:generate": "npx ts-node --project scripts/tsconfig.json scripts/generate-qr-codes.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.77.0",
    "@aws-sdk/client-s3": "^3.967.0",
    "@prisma/client": "^6.19.2",
    "@upstash/qstash": "^2.9.0",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "next": "^15.5.12",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^6.22.0",
    "prisma": "^6.19.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.2.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.14",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "husky": "^8.0.3",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "tailwindcss": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
